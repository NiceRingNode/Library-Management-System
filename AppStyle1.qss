QPushButton 
{
	border-style:solid;
	border-width:2px;	
	border-radius: 5px; /* 边框圆角 */
	border-color:#D5EB887;
	background-color: #FFE4C4; /* 背景颜色 */
	color: black; /* 文本颜色 */
	font-family: "宋体"; /* 文本字体族 */
	font-size: 10pt; /* 文本字体大小 */
}
QPushButton:pressed
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
	stop: 0 #dadbde, stop: 1 #f6f7fa);
}
QPushButton:hover
{
	background-color:#FFDAB9;
}